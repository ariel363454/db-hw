#include <iostream>
#include <cstring>
#include <string>
using namespace std;

int main(){
    int n, m;
    string name;
    //輸入人數和長度
    cout << "請輸入學生人數" << endl;
    cin >> n;
    cout << "請輸入最長姓名長度";
    cin >> m;
    //用new建立二維動態陣列
    char **names = new char*[n];
    for(int i = 0;i < n;i++){
        names[i] = new char[m+1];
        std::memset(names[i], 0, m+1);
    }
    //輸入學生姓名
    cout << "請輸入所有學生姓名 不同名字中間用空白格開" << endl;
    for(int i = 0;i < n;i++){
        cin >> names[i];
    }
    //依照最後一個字的ASCII順序排好
    for(int i = n; i > 0;i--){
        for(int j = 0;j < n-1;j++){
            if(names[j][strlen(names[j])-1] > names[j+1][strlen(names[j+1])-1]){
                char* temp;
                temp = names[j];
                names[j] = names[j+1];
                names[j+1] = temp;
            }
        }
    }
    //輸出排序好的結果
    for(int i = 0;i <n;i++){
        cout << names[i] << endl;
    }
    //釋放所佔用記憶體
    for(int i = 0;i < n;i++){
        delete [] names[i];
    }
    delete [] names;
    return 0;
}
