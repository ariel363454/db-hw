/* my homework */
#include <iostream>
#include <cstring>
#include <string>
using namespace std;

int main(){
    int n, m;
    string name;
    cout << "請輸入學生人數及最長姓名長度" << endl;
    cin >> n >> m;
    char **names = new char*[n];
    for(int i = 0;i < n;i++){
        names[i] = new char[m+1];
        std::memset(names[i], 0, m+1);
    }
    
    cout << "請輸入所有學生姓名 中間用空白格開" << endl;
    for(int i = 0;i < n;i++){
        cin >> names[i];
    }
    
    for(int i = n; i > 0;i--){
        for(int j = 0;j < n-1;j++){
            if(names[j][strlen(names[j])-1] > names[j+1][strlen(names[j+1])-1]){
                char* temp;
                temp = names[j];
                names[j] = names[j+1];
                names[j+1] = temp;
            }
        }
    }
    
    for(int i = 0;i <n;i++){
        cout << names[i] << endl;
    }

    for(int i = 0;i < n;i++){
        delete [] names[i];
    }
    delete [] names;
    return 0;
}
